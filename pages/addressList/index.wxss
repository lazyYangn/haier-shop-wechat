page {
  background-color: #f9f9f9;
}
.address_list_wrap {
  padding: 20rpx;
}
.address_list_wrap .address_item {
  background-color: #fff;
  height: 120rpx;
  display: flex;
  border-radius: 20rpx;
  margin-bottom: 20rpx;
}
.address_list_wrap .address_item .item_isDefault {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
}
.address_list_wrap .address_item .item_left {
  padding: 10rpx;
  flex: 6;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.address_list_wrap .address_item .item_left .left_title {
  font-size: 30rpx;
}
.address_list_wrap .address_item .item_right {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding: 10rpx;
}
.address_list_wrap .address_item .item_right view {
  display: flex;
  justify-content: center;
  align-items: center;
}
button {
  background-color: orangered;
  color: #fff;
  border-radius: 40rpx;
  position: fixed;
  width: 100%;
  bottom: 10rpx;
}
