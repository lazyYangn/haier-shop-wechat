Tabs .order_wrap {
  width: 100%;
  margin-top: 20rpx;
}
Tabs .order_wrap .order_list {
  background-color: #f9f9f9;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
Tabs .order_wrap .order_list image {
  margin-top: 40rpx;
  width: 200rpx;
}
Tabs .order_wrap .order_list .order_text {
  color: #e5e5e5;
}
Tabs .order_wrap .order_list_succ {
  background-color: #f9f9f9;
}
Tabs .order_wrap .order_list_succ .order_item {
  margin-bottom: 40rpx;
  display: flex;
  flex-direction: column;
  background-color: #fff;
  border-bottom: 2rpx solid #ddd;
}
Tabs .order_wrap .order_list_succ .order_item .order_title {
  height: 90rpx;
  padding: 10rpx 20rpx;
  margin-bottom: 20rpx;
  display: flex;
}
Tabs .order_wrap .order_list_succ .order_item .order_title .title_left {
  flex: 5;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
Tabs .order_wrap .order_list_succ .order_item .order_title .title_left .left_title {
  font-size: 32rpx;
  font-weight: 400;
}
Tabs .order_wrap .order_list_succ .order_item .order_title .title_left .left_content {
  font-size: 28rpx;
  color: #666666;
}
Tabs .order_wrap .order_list_succ .order_item .order_title .title_right {
  flex: 2;
  color: orangered;
  font-size: 32rpx;
}
Tabs .order_wrap .order_list_succ .order_item .order_content {
  background-color: #f9f9f9;
  padding: 10rpx;
  display: flex;
}
Tabs .order_wrap .order_list_succ .order_item .order_content .content_detail {
  flex: 2;
  display: flex;
  justify-content: center;
  align-items: center;
  color: orangered;
}
Tabs .order_wrap .order_list_succ .order_item .order_content .content_info {
  flex: 4;
  display: flex;
  justify-content: space-between;
  padding: 10rpx 20rpx;
}
Tabs .order_wrap .order_list_succ .order_item .order_content .content_info .main_price {
  font-size: 32rpx;
  font-weight: 600;
  color: orangered;
}
Tabs .order_wrap .order_list_succ .order_item .order_tools {
  height: 90rpx;
  padding: 20rpx;
  display: flex;
  margin-left: 360rpx;
  align-items: center;
}
Tabs .order_wrap .order_list_succ .order_item .order_tools .btn {
  width: 350rpx;
  display: flex;
  justify-content: flex-end;
}
Tabs .order_wrap .order_list_succ .order_item .order_tools .btn .btn_cancel {
  width: 160rpx;
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border: 1px solid #eee;
}
Tabs .order_wrap .order_list_succ .order_item .order_tools .btn .cancel {
  width: 160rpx;
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #ddd;
  color: #fff;
  margin-right: 10rpx;
}
Tabs .order_wrap .order_list_succ .order_item .order_tools .btn .btn_submit {
  width: 160rpx;
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: orangered;
  color: #fff;
}
Tabs .order_wrap .order_list_succ .order_item .order_tools .btn .submit {
  width: 220rpx;
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: orangered;
  color: #fff;
  margin-right: 10rpx;
}
Tabs .order_wrap .order_list_succ .order_item .order_tools .btn .isdisabled {
  width: 160rpx;
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #ddd;
  color: #fff;
}
.recommend_list {
  background-color: #f9f9f9;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.recommend_list .recommend_title {
  margin-top: 20rpx;
  font-size: 36rpx;
}
.recommend_list .recommend_project .project_show {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-wrap: wrap;
  padding: 10rpx;
}
.recommend_list .recommend_project .project_show .project_item {
  background-color: #fff;
  margin: 10rpx 0rpx 0rpx 10rpx;
  width: 48%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  padding: 10rpx 0;
}
.recommend_list .recommend_project .project_show .project_item .project_img {
  flex: 6;
}
.recommend_list .recommend_project .project_show .project_item .project_text {
  flex: 2;
  color: #474747;
  padding: 0 10rpx;
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.recommend_list .recommend_project .project_show .project_item .project_price {
  margin-top: 10rpx;
  flex: 2;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding: 10rpx;
  color: #ff6f6f;
  font-size: 32rpx;
}
