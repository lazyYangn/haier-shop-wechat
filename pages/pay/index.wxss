page {
  background-color: #f9f9f9;
}
.address_wrap {
  margin-bottom: 20rpx;
}
.address_wrap view .address_content1 {
  height: 90rpx;
  padding: 10rpx 20rpx;
  background-color: #fff;
  display: flex;
}
.address_wrap view .address_content1 .address_left {
  flex: 1;
}
.address_wrap view .address_content1 .address_left .address_icon {
  width: 70rpx;
  height: 70rpx;
  text-align: center;
  line-height: 65rpx;
  background-color: #3388ff;
  border-radius: 10rpx;
  color: #fff;
  font-size: 70rpx;
}
.address_wrap view .address_content1 .address_info {
  flex: 4;
  display: flex;
  justify-content: center;
}
.address_wrap view .address_content1 .address_info .address_tools {
  width: 100%;
  text-align: left;
  padding-top: 10rpx;
}
.address_wrap view .address_content1 .address_right {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
  text-align: center;
}
.address_wrap .address_content {
  background-color: #fff;
  height: 90rpx;
  display: flex;
}
.address_wrap .address_content .address_left {
  flex: 1;
  text-align: center;
  line-height: 90rpx;
}
.address_wrap .address_content .address_info {
  flex: 6;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.address_wrap .address_content .address_info .adderss_tools {
  display: flex;
  justify-content: space-between;
}
.address_wrap .address_content .address_info .adderss_tools .address_user {
  font-weight: 800;
  font-size: 30rpx;
}
.address_wrap .address_content .address_info .adderss_tools .address_tel {
  font-weight: 800;
  font-size: 30rpx;
}
.address_wrap .address_content .address_right {
  flex: 1;
  text-align: center;
  line-height: 90rpx;
}
.cart_content view.cart_title {
  display: flex;
  justify-content: space-between;
  padding: 20rpx;
  font-size: 30rpx;
  margin-bottom: 20rpx;
  background-color: #fff;
}
.cart_content .cart_main {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.cart_content .cart_main .cart_item {
  border-radius: 10rpx;
  background-color: #f9f9f9;
  display: flex;
  padding: 20rpx 10rpx;
  margin-bottom: 15rpx;
}
.cart_content .cart_main .cart_img_wrap {
  flex: 2;
  display: flex;
  justify-content: center;
  align-items: center;
}
.cart_content .cart_main .cart_img_wrap image {
  width: 80%;
}
.cart_content .cart_main .cart_info_wrap {
  flex: 4;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.cart_content .cart_main .cart_info_wrap view.goods_name {
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  color: #666;
}
.cart_content .cart_main .cart_info_wrap .goods_price_wrap {
  display: flex;
  justify-content: space-between;
}
.cart_content .cart_main .cart_info_wrap .goods_price_wrap .goods_price_info {
  display: flex;
  color: #666;
  justify-content: center;
  align-items: center;
}
.cart_content .cart_main .cart_info_wrap .goods_price_wrap .goods_price_info .goods_price {
  font-size: 40rpx;
  color: orangered;
  font-weight: 600;
  margin-right: 10rpx;
}
.cart_content .cart_main .cart_info_wrap .goods_price_wrap .goods_price_info .goods_price_originalPrice {
  font-size: 30rpx;
  text-decoration: line-through;
}
.cart_content .cart_main .cart_info_wrap .goods_price_wrap .cart_num_tool {
  margin-right: 20rpx;
  display: flex;
}
.cart_content .cart_main .cart_info_wrap .goods_price_wrap .cart_num_tool .goods_num {
  width: 55rpx;
  height: 55rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.cart_content .cart_tools {
  width: 100%;
}
.cart_content .cart_tools .cart_tools_wrap {
  width: 100%;
  margin-bottom: 20rpx;
}
.cart_content .cart_tools .cart_tools_wrap .tools_item {
  display: flex;
  justify-content: space-between;
  background-color: #fff;
  border-bottom: 2rpx #f9f9f9 solid;
  height: 90rpx;
  font-size: 30rpx;
}
.cart_content .cart_tools .cart_tools_wrap .tools_item .tools_left {
  line-height: 90rpx;
  padding: 0 30rpx;
}
.cart_content .cart_tools .cart_tools_wrap .tools_item .tools_right {
  line-height: 90rpx;
  padding: 0 30rpx;
}
.cart_content .cart_tools .cart_tools_wrap .tools_item .tools_right text {
  color: red;
}
.cart_content .cart_tools .cart_tools_wrap .talk {
  display: flex;
}
.cart_content .cart_tools .cart_tools_wrap .talk .tools_left {
  flex: 2;
}
.cart_content .cart_tools .cart_tools_wrap .talk input {
  flex: 6;
  margin-top: 20rpx;
}
.footer_tools {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 90rpx;
  background-color: #fff;
  display: flex;
}
.footer_tools .total_price_wrap {
  line-height: 90rpx;
  flex: 4;
  padding-right: 15rpx;
  text-align: right;
}
.footer_tools .total_price_wrap .total_price .total_price_text {
  font-size: 34rpx;
  font-weight: 600;
}
.footer_tools .order_pay_wrap {
  flex: 3;
  background-color: red;
  border-radius: 40rpx;
  color: #fff;
  font-size: 32rpx;
  font-weight: 600;
  display: flex;
  justify-content: center;
  align-items: center;
}
