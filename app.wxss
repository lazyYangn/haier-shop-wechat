@import './styles/iconfont/iconfont.wxss';
/* 在微信小程序中不支持通配符 */
page,
view,
text,
swiper,
swiper-item,
image,
navigator {
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}
/* 主题颜色 */
page {
  --themeColor: #ff4444;
  /* 定义统一的字体大小 */
  font-size: 28rpx;
}
image {
  width: 100%;
}
